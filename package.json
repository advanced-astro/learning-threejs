{
  "name": "learning-threejs",
  "description": "A simple Astro.js project for learning Three.js with GSAP, Tailwind CSS and Bun.",
  "type": "module",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/estubmo/learning-threejs.git"
  },
  "author": "Eirik Mo <eirk@mowebdev.com> (http://www.mowebdev.com/)",
  "keywords": [
    "astro",
    "threejs",
    "typescript",
    "tailwindcss",
    "gsap",
    "bun"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "start": "bunx --bun astro dev",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview",
    "astro": "astro",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\""
  },
  "dependencies": {
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/tailwind": "^5.0.2",
    "astro": "^3.3.0",
    "gsap": "^3.12.2",
    "tailwindcss": "^3.0.24",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/three": "^0.157.0",
    "@typescript-eslint/parser": "^6.7.5",
    "bun-types": "^1.0.6",
    "eslint": "^8.51.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^3.0.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "rehype-pretty-code": "^0.10.1"
  }
}
